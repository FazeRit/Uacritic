{
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleDetection": "force",
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true
  }
}